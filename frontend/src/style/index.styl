*
    margin: 0px
    padding: 0px
    box-sizing: border-box

body
    padding: 20px
    font-size: 20px

.parser-container
    
    textarea
        
        width: 100%
        height: 300px


    .error
        color: red
        
    table
        margin-top: 30px

        td, th
            padding: 3px 6px
            
        border-collapse: collapse
        
        th
            background-color: #E6E6E6
            
        th + th, td + td
            border-left: solid 1px gray